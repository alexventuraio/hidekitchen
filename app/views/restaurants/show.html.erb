<div class="page-header">
  <%= link_to restaurants_path, class: 'btn btn-default' do %>
    All Restaurants
  <% end %>
  <%= link_to edit_restaurant_path(@restaurant), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show restaurant</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @restaurant.name %></dd>

  <dt>Domain:</dt>
  <dd><%= @restaurant.domain %></dd>

  <dt>Subdomain:</dt>
  <dd><%= @restaurant.subdomain %></dd>

  <dt>User:</dt>
  <dd><%= @restaurant.user.email %></dd>
</dl>
